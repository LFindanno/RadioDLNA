config wifi-device  wlan0
	option type     mac80211
	option channel  11
	option hwmode	11g
	option path	'platform/ar933x_wmac'
	option htmode	HT20
	# REMOVE THIS LINE TO ENABLE WIFI:
	#option disabled 1

config wifi-iface
	option device   wlan0
	option network  lan
	option mode     sta
	option ssid     'xxxxx'
	option encryption 'psk2'
	option key 'xxxxx'

